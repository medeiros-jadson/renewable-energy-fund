{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": ">=16.0"
      }
    ],
    "Watchman": [
      {
        "rule": "cli",
        "binary": "watchman",
        "error": "Please install watchman on this machine. Refer to the official Watchman installation instructions for additional help.",
        "platform": [
          "darwin",
          "linux"
        ]
      }
    ],
    "Package JSON": [
      {
        "rule": "file",
        "location": "./package.json"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": ">=7.0"
      }
    ],
    "TypeScript": [
      {
        "rule": "cli",
        "binary": "tsc",
        "semver": ">=3.5"
      }
    ],
    "Yarn": [
      {
        "rule": "cli",
        "binary": "yarn",
        "version": "--version",
        "semver": ">=1.20.0"
      }
    ],
    "CocoaPods": [
      {
        "rule": "cli",
        "binary": "pod",
        "platform": "darwin",
        "semver": ">=1.11"
      },
      {
        "rule": "dir",
        "location": "./ios/Pods"
      }
    ],
    "Android": [
      {
        "rule": "env",
        "variable": "ANDROID_HOME",
        "error": "The ANDROID_HOME environment variable must be set to your local SDK.  Refer to getting started docs for help."
      }
    ],
    "Xcode": [
      {
        "rule": "cli",
        "binary": "xcodebuild",
        "semver": ">=12.0",
        "platform": "darwin"
      },
      {
        "rule": "cli",
        "binary": "xcrun",
        "semver": ">=48",
        "platform": "darwin"
      }
    ]
  }
}
